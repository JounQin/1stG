{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false
      }
    ],
    [
      "@babel/stage-0",
      {
        "decoratorsLegacy": true
      }
    ],
    "@babel/react"
  ],
  "plugins": ["react-hot-loader/babel"],
  "env": {
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "removeImport": true
          }
        ]
      ]
    }
  }
}
