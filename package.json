{
  "name": "1stg",
  "version": "0.0.0",
  "description": "Server Rendered Static Homepage Website powered by Technology Stack Of React",
  "repository": "git@github.com:JounQin/1stg.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "build": "rimraf dist && cross-env DEBUG=1stg:* NODE_ENV=production run-p build:**",
    "build:react:client": "webpack --config=build/react.client.babel.js --colors --progress",
    "build:react:server": "webpack --config=build/react.server.babel.js --colors --progress",
    "build:server": "webpack --config build/server.babel.js --colors --progress",
    "build:template": "node build/template.js",
    "dev": "cross-env DEBUG=1stg:* babel-node server",
    "lint": "cross-env EFF_NO_LINK_RULES=true eslint . --cache --max-warnings=10 -f friendly",
    "now-build": "sh build/now.sh",
    "postinstall": "yarn-deduplicate || exit 0",
    "start": "cross-env DEBUG=1stg:* node dist/server"
  },
  "dependencies": {
    "anujs": "^1.6.0",
    "core-js": "^3.6.4",
    "cross-env": "^6.0.3",
    "debug": "^4.1.1",
    "koa": "^2.11.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.1",
    "koa-static-cache": "^5.1.2",
    "memory-fs": "^0.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-server-renderer": "^1.2.0"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.1.21",
    "@babel/node": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@hot-loader/react-dom": "^16.11.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "eslint-import-resolver-webpack": "^0.12.1",
    "file-loader": "^5.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-minifier": "^4.0.0",
    "img-loader": "^3.0.1",
    "koa-webpack": "^5.2.5",
    "mini-css-extract-plugin": "^0.9.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "postcss-plugin-px2rem": "^0.8.1",
    "prop-types": "^15.7.2",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "react-hot-loader": "^4.12.19",
    "react-style-loader": "^2.3.0",
    "rimraf": "^3.0.0",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2",
    "yarn-deduplicate": "^1.1.1"
  },
  "resolutions": {
    "@babel/core": "^7.8.3"
  }
}
